{
  "name": "@vue-formily/util",
  "version": "0.1.0",
  "description": "Common JavaScript utility library with performance, lightweight.",
  "author": "An Ha",
  "license": "MIT",
  "module": "esm/index.js",
  "unpkg": "dist/vue-formily-util.js",
  "main": "dist/vue-formily-util.js",
  "types": "dist/vue-formily-util.d.ts",
  "homepage": "https://github.com/vue-formily/util",
  "repository": "https://github.com/vue-formily/util",
  "sideEffects": false,
  "keywords": [
    "utility"
  ],
  "files": [
    "dist",
    "esm"
  ],
  "scripts": {
    "test": "NODE_ENV=test jest",
    "cover": "jest --coverage",
    "build": "node scripts/build.js",
    "lint": "eslint . '**/*.{js,jsx,ts,tsx}' --fix",
    "format": "prettier \"./**/*.ts\" --write"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.7",
    "eslint": "^7.15.0",
    "filesize": "^6.1.0",
    "fs-extra": "^9.0.1",
    "gzip-size": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "rollup": "^2.34.2",
    "rollup-plugin-dts": "^2.0.0",
    "rollup-plugin-typescript2": "^0.29.0",
    "shelljs": "^0.8.4",
    "terser": "^5.5.1",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.2"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write",
      "jest --maxWorkers=1 --bail --findRelatedTests"
    ],
    "*.js": [
      "eslint --fix",
      "jest --maxWorkers=1 --bail --findRelatedTests"
    ]
  }
}
